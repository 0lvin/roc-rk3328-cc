/dts-v1/;

#include "rk3188.dtsi"
#include "rk3188-clocks.dtsi"
#include "lcd-b101ew05.dtsi"

/ {
	memory {
		device_type = "memory";
		reg = <0x60000000 0x80000000>;
	};

	chosen {
		bootargs = "clk_ignore_unused";
	};

	fiq-debugger {
		serial-id = <2>;
		status = "okay";
	};
};

&uart0 {
	status = "okay";
};

&uart3 {
	status = "okay";
};

&i2c0 {
	status = "okay";
};

&i2c1 {
	status = "okay";
	rtc@51 {
		compatible = "nxp,pcf8563";
		reg = <0x51>;
	};
};

&i2c3 {
	status = "okay";
};

&fb {
	rockchip,disp-mode = <DUAL>;
};

&lcdc0 {
	status = "okay";
	power_ctr = <&disp_power_ctr>;
	display-timings = <&disp_timings>;
};

&lcdc1 {
	status = "okay";
};
&pwm3 {
        status = "okay";
};

